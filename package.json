{
  "name": "epfc-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@date-io/date-fns": "2.10.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@mdi/js": "^5.0.45",
    "@mdi/react": "^1.3.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.11.1",
    "graphql": "^15.5.0",
    "graphql.macro": "^1.4.2",
    "history": "^5.0.0",
    "lodash": "^4.17.15",
    "nanoid": "^3.0.0",
    "node-gyp": "^7.0.0",
    "react": "^17.0.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.4",
    "react-number-format": "^4.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1",
    "react-text-mask": "^5.4.3",
    "tronweb": "^3.2.3",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "types": "env-cmd -f .env.development graphql-codegen",
    "start": "env-cmd -f .env.development react-app-rewired start",
    "start:dev": "env-cmd -f .env.development react-app-rewired start",
    "build": "env-cmd -f .env.development react-app-rewired build",
    "build:dev": "env-cmd -f .env.development react-app-rewired build",
    "eject": "react-app-rewired eject",
    "precommit": "lint-staged",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test": "react-app-rewired test",
    "lint": "node_modules/.bin/eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx  --ignore-path .gitignore src/",
    "lint:fix": "node_modules/.bin/eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx  --ignore-path .gitignore --fix src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-text-mask": "^5.4.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "env-cmd": "^10.1.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "node-sass": "4.14.1",
    "react-app-rewired": "^2.1.5",
    "react-hot-loader": "^4.12.19"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": [
      "node_modules/.bin/eslint --fix --max-warnings=0",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
